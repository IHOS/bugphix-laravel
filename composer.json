{
    "name": "ihos/bugphix-laravel",
    "description": "Capture and monitor detailed error logs with nice dashboard and UI.",
    "keywords": [
        "bugphix",
        "bugphix-laravel",
        "laravel-bugphix",
        "laravel-error",
        "error",
        "error-catcher",
        "error-handler",
        "error-manager",
        "error-monitoring",
        "error-watcher",
        "error-viewer",
        "error-logger",
        "ihos"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vinicius Assis Nascimento",
            "email": "viniciusassisnascimento@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Bugphix\\BugphixLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bugphix\\BugphixLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-1.5.5": "1.5.6.x-dev"
        },
        "laravel": {
            "providers": [
                "Bugphix\\BugphixLaravel\\BugphixServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.4|8.0",
        "guzzlehttp/guzzle": "^6.5|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "laravel/laravel": "^7.0|^8.0",
        "orchestra/testbench": "^6.0"
    }
}
